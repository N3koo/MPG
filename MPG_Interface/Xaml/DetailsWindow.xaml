<Window x:Class="MPG_Interface.Xaml.DetailsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Detail" Height="800" Width="600" WindowState="Normal" ResizeMode="CanResize">

    <FlowDocumentReader ViewingMode="Scroll">
        <FlowDocument x:Name="fdViewer">
            <FlowDocument.Resources>
            </FlowDocument.Resources>
            <Table x:Name="tbDetails">
                <Table.Columns>
                    <TableColumn/>
                    <TableColumn/>
                    <TableColumn/>
                </Table.Columns>
            </Table>
        </FlowDocument>
    </FlowDocumentReader>
</Window>
